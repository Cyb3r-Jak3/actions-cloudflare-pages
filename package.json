  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "axios": "^1.6.0",
    "bent": "^7.3.12",
    "yarn": "^1.22.13"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
